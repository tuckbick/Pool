<body>
	<div id="content">
		<div class="row" id="header">
			<div class="fixed contain-to-grid">
				<nav class="top-bar">
					<ul>
						<li class="name">
							<h1 class="title show"><%= title %></h1>
							<h2 class="status"></h2>
						</li>
						<li class="toggle-topbar show"><a href="#"></a></li>
					</ul>
					<section>
						<ul class="right">
							<li class="divider"></li>
							<li>
								<a id="add-photos" href="#">Add Photos</a>
								<input id="file-input" class="hide" type="file" name="file-photos[]" accept="image/*" multiple />
							</li>
						</ul>
					</section>
				</nav>
			</div>
		</div>
		<div class="row" id="loading">
			<div class="twelve columns">
				<img src="images/loading.gif" />
			</div>
		</div>
		<div class="row" id="photos">
			<div class="column">
				<ul class="block-grid eight-up" id="photos-list"></ul>
			</div>
		</div>
		<div id="darkroom">
			<div class="stage">
				<img src />
			</div>
			<ul id="controls">
				<li><a href="#">&nbsp;<!-- <i class="foundicon-left-arrow"></i> --></a></li>
				<li><a href="#" class="go-home"><i class="foundicon-home"></i></a></li>
				<li><a href="#">&nbsp;<!-- <i class="foundicon-right-arrow"></i> --></a></li>
			</ul>
		</div>
	</div>

	<div id="templates" style="display: none;">
		<script id="thumb-loading" type="text/template">
			<li><img src="images/loading_sml_img.gif" /></li>
		</script>
		<script id="thumb" type="text/template">
			<li><img src="{{ src }}" /></li>
		</script>
	</div>
	
	<script src="/socket.io/socket.io.js"></script>
	<script src="javascripts/libs/fastclick.js"></script>
	<script src="javascripts/libs/keyboard.min.js"></script>
	<script src="javascripts/libs/jquery.min.js"></script>
